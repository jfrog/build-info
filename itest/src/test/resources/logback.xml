<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>


    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>


    <!--Logger for Artifactory Java Client and Docker rest client-->
    <logger name="groovyx.net.http">
        <level value="DEBUG"/>
    </logger>

</configuration>